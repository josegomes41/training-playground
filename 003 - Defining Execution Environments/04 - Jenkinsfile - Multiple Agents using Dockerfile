
pipeline {
    agent none
    stages {
     stage ('maven'){
     	 agent {dockerfile {
	 	filename ''
		dir
		}}
        	steps{
    	    		 sh 'mvn --version'
            	}
         }
     stage ('npm'){
     	 agent {dockerfile {
	 	filename ''
		dir ''
		}}
          		 steps{
    	      			  sh 'npm --version'
            		 }
             }
    }
    }
