pipeline {
    agent none
    stages {
     stage ('maven'){
     	 agent {
    	docker {
        image 'maven:latest'
    }
}
    	 sh 'mvn --version'
    }
     stage ('npm'){
     	 agent {
    	docker {
        image 'node:latest'
    }
    	 sh 'npm --version'
    }
    }
    }}
    
